	<link rel="stylesheet" href="css/main.min.css">
	<h1>Word Cloud</h1>
		<hr>
		<% @wordlist.each do |word| %>  
			<% if word[1] == @least_frequent %> 
				<li class="one"> <%= word[0]  %> </li>
			<% elsif word[1] > @least_frequent && word[1] < @mean %> 
				<li class="two"> <%= word[0]  %> </li>
			<% elsif word[1] <= @mean %> 
				<li class="three"> <%= word[0]  %> </li>
			<% elsif word[1] > @mean && word[1] < @most_frequent %> 
				<li class="four"> <%= word[0]  %> </li>
			<% else %> 
				<li class="five"> <%= word[0] %> </li>
			<% end %> 
		<% end %>
